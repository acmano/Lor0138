<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/ConsultaEstoqueSaldoLocal"
  android:layout_width="wrap_content"
  android:layout_height="25dp"
  android:orientation="horizontal"
  android:weightSum="65"
  android:baselineAligned="false"
  android:paddingLeft="5dip"
  android:paddingRight="5dip"
>
  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:orientation="horizontal"
  >
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="match_parent"
      android:layout_weight="1"
    >
      <View
        android:layout_width="0dp"
        android:layout_height="match_parent"
      />
    </LinearLayout>
    <!-- 01 - Espaço -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="10"
    >
      <TextView
        android:id="@+id/txtConsultaEstoqueSaldoLocalCodigoLocal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:typeface="monospace"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:gravity="left|center_vertical"
        android:background="#ffffff"
      />
    </LinearLayout>
    <!-- 10 - txtConsultaEstoqueSaldoLocalCodigoLocal -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="1"
    >
      <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
      />
    </LinearLayout>
    <!-- 01 - Espaço -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="19"
    >
      <TextView
        android:id="@+id/txtConsultaEstoqueSaldoLocalNumeroLote"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:typeface="monospace"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:gravity="left|center_vertical"
        android:background="#ffffff"
      />
    </LinearLayout>
    <!-- 19 - txtConsultaEstoqueSaldoLocalNumeroLote -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="1"
    >
      <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
      />
    </LinearLayout>
    <!-- 01 - Espaço -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="2"
    >
      <TextView
        android:id="@+id/txtConsultaEstoqueSaldoLocalSituacao"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:typeface="monospace"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:gravity="center|center_vertical"
        android:background="#ffffff"
      />
    </LinearLayout>
    <!-- 02 - txtConsultaEstoqueSaldoLocalSituacao -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="1"
    >
      <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
      />
    </LinearLayout>
    <!-- 01 - Espaço -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="19"
    >
      <TextView
        android:id="@+id/txtConsultaEstoqueSaldoLocalQuantidade"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:typeface="monospace"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:gravity="right|center_vertical"
        android:background="#ffffff"
      />
    </LinearLayout>
    <!-- 19 - txtConsultaEstoqueSaldoLocalQuantidade -->
    <LinearLayout
      android:orientation="horizontal"
      android:gravity="center_vertical"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_weight="1"
    >
      <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
      />
    </LinearLayout>
    <!-- 01 - Espaço -->
  </LinearLayout>
</RelativeLayout>